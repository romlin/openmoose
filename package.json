{
  "name": "openmoose",
  "version": "0.1.1",
  "description": "OpenMoose: A privacy-first, local-first AI assistant gateway.",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "author": "OpenMoose Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenMoose/openmoose.git"
  },
  "homepage": "https://github.com/OpenMoose/openmoose#readme",
  "bugs": {
    "url": "https://github.com/OpenMoose/openmoose/issues"
  },
  "keywords": [
    "ai",
    "assistant",
    "local-first",
    "privacy",
    "whatsapp",
    "llm",
    "tts",
    "voice"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "gateway": "tsx src/gateway/server.ts",
    "auth": "tsx src/cli/whatsapp-auth.ts",
    "build": "tsc && mkdir -p dist/runtime/browser && cp src/runtime/browser/daemon.js src/runtime/browser/Dockerfile dist/runtime/browser/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:gateway": "bash scripts/bundle-gateway.sh",
    "setup:linux": "bash scripts/setup-linux.sh",
    "clean": "rm -rf dist app/dist app/src-tauri/target/release/bundle app/src-tauri/resources/gateway",
    "dist": "pnpm clean && pnpm build:gateway && cd app && pnpm tauri build --bundles deb,rpm"
  },
  "dependencies": {
    "@hapi/boom": "10.0.1",
    "@hono/node-server": "1.19.9",
    "@huggingface/transformers": "3.8.1",
    "@lancedb/lancedb": "0.26.2",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "apache-arrow": "18.1.0",
    "chalk": "5.6.2",
    "commander": "14.0.3",
    "cron-parser": "5.5.0",
    "dotenv": "17.3.0",
    "hono": "4.11.8",
    "js-yaml": "4.1.1",
    "node-llama-cpp": "3.15.1",
    "onnxruntime-common": "1.21.0",
    "onnxruntime-node": "1.21.0",
    "ora": "9.3.0",
    "pino": "10.3.0",
    "qrcode": "1.5.4",
    "qrcode-terminal": "0.12.0",
    "ws": "8.19.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "25.2.1",
    "@types/qrcode": "1.5.6",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.21.0",
    "tsx": "4.21.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "vitest": "4.0.18"
  },
  "pnpm": {
    "overrides": {
      "tar": ">=7.5.7",
      "onnxruntime-node": "1.21.0",
      "onnxruntime-common": "1.21.0"
    }
  }
}